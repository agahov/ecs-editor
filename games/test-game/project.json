{
  "name": "test-game",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "games/test-game/src",
  "projectType": "application",
  "targets": {
    "build": {
      "executor": "@nx/vite:build",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/games/test-game",
        "configFile": "games/test-game/vite.config.ts"
      }
    },
    "dev": {
      "executor": "@nx/vite:dev-server",
      "options": {
        "buildTarget": "test-game:build"
      }
    },
    "preview": {
      "executor": "@nx/vite:preview-server",
      "options": {
        "buildTarget": "test-game:build"
      }
    }
  },
  "tags": ["type:app", "scope:game"],
  "implicitDependencies": ["ecs-core", "aspects-core", "canvas-runtime"]
}

